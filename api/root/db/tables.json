{
  "tables": [
    {
      "table_name": "users",
      "columns": [
        "uid VARCHAR PRIMARY KEY",
        "email TEXT UNIQUE",
        "password_hash TEXT",
        "user_name TEXT",
        "phone TEXT",
        "is_active INT",
        "email_verified INT",
        "role INT",
